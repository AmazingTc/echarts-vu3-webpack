        data = res.data\r\n        }\r\n\r\n        //图表初始化\r\n        function chartInit() {\r\n            // 初始化echarts\r\n            let myChart = $charts.init(document.querySelector('#twoChart'))\r\n            getData().then(() => {\r\n                setData()\r\n                myChart.setOption({\r\n                    tooltip: {\r\n                        trigger: 'axis'\r\n                    },\r\n                    legend: {\r\n                        data: ['前端开发', '后端开发', '大数据测试','软件工程师'], \r\n                        textStyle:{\r\n                            color:'#00BFFF'\r\n                        }  \r\n                    },\r\n                    grid: {\r\n                        left: '2%',\r\n                        right: '4%',\r\n                        bottom: '10%',\r\n                        top: '20%',\r\n                        containLabel: true,//包含刻度文字在内\r\n                        borderColor:'none'\r\n                    },\r\n                    xAxis: {\r\n                        type: 'category',\r\n                        boundaryGap: false,\r\n                        data: ['1月', '3月', '5月', '7月', '9月', '11月'],\r\n                        axisLabel: {\r\n                            color: \"#fff\"\r\n                        },\r\n                    },\r\n                    yAxis: {\r\n                        type: 'value',\r\n                        splitLine:{\r\n                            lineStyle:{\r\n                                color:\"rgba(255,255,255,.1)\"\r\n                            }\r\n                        },\r\n                        axisLabel: {\r\n                            color: \"#fff\"\r\n                        },\r\n                    },\r\n                    series: [\r\n                        {\r\n                            name: '前端开发',\r\n                            type: 'line',\r\n                            stack: 'Total',\r\n                            data: [120, 132, 101, 134, 90, 230, 210],\r\n                            itemStyle: {\r\n                                color: 'red'\r\n                            }\r\n                        },\r\n                        {\r\n                            name: '后端开发',\r\n                            type: 'line',\r\n                    \r\n                            data: [220, 182, 191, 234, 290, 330, 310],\r\n                            itemStyle: {\r\n                                color: 'pink'\r\n                            }\r\n                        },\r\n                        {\r\n                            name: '大数据测试',\r\n                            type: 'line',\r\n             \r\n                            data: [150, 232, 201, 154, 190, 330, 410],\r\n                            itemStyle: {\r\n                                color: 'green'\r\n                            }\r\n                        },\r\n                        {\r\n                            name: '软件工程师',\r\n                            type: 'line',\r\n\r\n                            data: [123, 232, 201, 239, 290, 280, 410],\r\n                            itemStyle: {\r\n                                color: 'purple'\r\n                            }\r\n                        },\r\n                    ]\r\n                })\r\n            })\r\n            // 图表大小自适应(监听窗口大小改变)\r\n            window.addEventListener('resize', () => {\r\n                myChart.resize()\r\n            })\r\n        }\r\n\r\n        onMounted(() => {\r\n            chartInit()\r\n        })\r\n\r\n        return {\r\n            getData, data, xdata, ydata,\r\n            setData\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style lang=\"less\" scoped>\r\nh2 {\r\n    font-size: .25rem;\r\n    color: #fff;\r\n    height: .6rem;\r\n    text-align: center;\r\n}\r\n\r\n.chart {\r\n    height: 3.5rem\r\n        /* 280px -> 3.5rem */\r\n    ;\r\n    width: 100%;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}