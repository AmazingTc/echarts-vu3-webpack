yle: {\n                  fill: '#E0FFFF'\n                }\n              }]\n            }\n          }]\n        }\n      };\n      var myChart = $charts.init(document.querySelector(\".test\"));\n      myChart.setOption(option);\n      window.addEventListener('resize', function () {\n        myChart.resize();\n      });\n    }\n\n    // 获取时间\n    function getTime() {\n      data = new Date();\n      year.value = data.getFullYear();\n      month.value = data.getMonth() + 1;\n      date.value = data.getDate();\n    }\n    onMounted(function () {\n      chartInit();\n      getTime();\n    });\n    return {\n      year: year,\n      month: month,\n      date: date,\n      getTime: getTime\n    };\n  }\n};","map":{"version":3,"mappings":";AAkDA,SAASA,MAAM,EAAEC,SAAS,EAAEC,GAAE,QAAS,KAAI;AAC3C,OAAOC,UAAS,MAAO,8BAA6B;AACpD,OAAOC,UAAS,MAAO,8BAA6B;AACpD,OAAOC,YAAW,MAAO,gCAA+B;AACxD,OAAOC,WAAU,MAAO,+BAA8B;AACtD,OAAOC,SAAQ,MAAO,gCAA+B;AACrD,OAAOC,UAAS,MAAO,8BAA6B;AACpD,eAAe;EACXC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE;IAAEF,UAAU,EAAVA,UAAU;IAACD,SAAS,EAATA,SAAS;IAAEJ,UAAU,EAAVA,UAAU;IAAEC,UAAU,EAAVA,UAAU;IAAEC,YAAY,EAAZA,YAAY;IAAEC,WAAW,EAAXA;EAAW,CAAC;EACtFK,KAAK,mBAAG;IACJ,IAAIC,OAAM,GAAIZ,MAAM,CAAC,SAAS;IAC9B,IAAIa,MAAK,GAAIb,MAAM,CAAC,OAAO;IAE3B,IAAIc,IAAG,GAAIZ,GAAG,CAAC,EAAE;IACjB,IAAIa,IAAG,GAAIb,GAAG,CAAC,EAAE;IACjB,IAAIc,KAAI,GAAId,GAAG,CAAC,EAAE;IAClB,IAAIe,IAAG,GAAIf,GAAG,CAAC,EAAE;IAEjB,SAASgB,SAAS,GAAE;MACpB;MACA,IAAIC,MAAK,GAAI;QACTC,OAAO,EAAE;UACLC,IAAI,EAAC;YACDC,YAAY,EAAC;UACjB,CAAC;UACDC,QAAQ,EAAE,CACN;YACIC,IAAI,EAAE,MAAM;YACZC,IAAI,EAAE,QAAQ;YACdC,GAAG,EAAE,QAAQ;YACbC,KAAK,EAAE;cACHC,IAAI,EAAE,SAAS;cACfC,QAAQ,EAAE,EAAE;cACZC,UAAU,EAAE,MAAM;cAClBC,QAAQ,EAAE,CAAC,CAAC,EAAE,GAAG,CAAC;cAClBC,cAAc,EAAE,CAAC;cACjBC,IAAI,EAAE,aAAa;cACnBC,MAAM,EAAE,SAAS;cACjBC,SAAS,EAAE;YACf,CAAC;YACDC,iBAAiB,EAAE;cACfC,QAAQ,EAAE,IAAI;cAEdC,SAAS,EAAE,CACP;gBACIC,OAAO,EAAE,GAAG;gBACZZ,KAAK,EAAE;kBACHM,IAAI,EAAE,aAAa;kBACnBD,cAAc,EAAE,GAAG;kBACnBD,QAAQ,EAAE,CAAC,GAAG,EAAE,CAAC;gBACrB;cAC